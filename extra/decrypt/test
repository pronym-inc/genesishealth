#!/usr/bin/env zsh

key=$(sudo cat /etc/mmkey)

input=('ExtensionID  3F55CC212EC949BB294D3589DA0E3283',
       'Hour         2DFA418CBEC78F9D5E535A1DC2561130',
       'GatewayType  DD9122696BE551ACAEA53D1CB5CF1575',
       'DataType     ADFF454907A2983D868D0CD895DB97A1',
       'Second       A1BC2C25A97386E8F41FD254F63AE584',
       'Month        C466564DAD8FAE1DC3403E50D86B0DAD',
       'Value6       B3C2061D5A80F0FC9BFF0F06126565CF',
       'Value5       B3C2061D5A80F0FC9BFF0F06126565CF',
       'Value4       B3C2061D5A80F0FC9BFF0F06126565CF',
       'DeviceType   DD9122696BE551ACAEA53D1CB5CF1575',
       'DeviceID     650179D518BDAF3615BBB93FA89A15FE',
       'Value1       B3C2061D5A80F0FC9BFF0F06126565CF',
       'Year         3D75AF12F7EAB46764B725CE95DBEBA9',
       'GatewayID    650179D518BDAF3615BBB93FA89A15FE',
       'Value3       B3C2061D5A80F0FC9BFF0F06126565CF',
       'Day          57D70E813514985A8FE888FE190E7544',
       'Minute       5345AD478A708B2E2AC9885ECAE636A6',
       'Value2       B3C2061D5A80F0FC9BFF0F06126565CF');

for i in $input; do
  s=(${(s: :)i})  
  dec=$(./decrypt $s[2] $key)
  echo $s[1] $dec
done
